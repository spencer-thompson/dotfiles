      "label": "󰖀 "
    },
    "backlight": {
      "label": "󰃟 ",
      "device": "amdgpu_bl1"
    },
    "menubar": {
      "menu#power": {
        "label": "Power",
        "position": "right",
        "actions": [
          {
            "label": "Shut down",
            "command": "systemctl poweroff"
          },
          {
            "label": " 󰜉 ",
            "command": "systemctl reboot"
          }
        ]
      },
      "buttons#power": {
        "position": "left",
        "actions": [
          {
            "label": "Screenshot",
